{
    "name": "StuddyBuddy",
    "version": "1.0",
    "description": "Healthy Study Helper",
    "manifest_version": 2,
    "icons": {
        "16": "16.png",
        "48": "48.png",
        "128": "128.png"},
    "content_scripts": [
        {
            "matches": [ "http://*.co.il/*"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },

    "browser_action": {
        "default popup": "popup.html"}
      }
  